{
  "name": "@melfore/mosaic",
  "version": "9.9.0",
  "homepage": "https://github.com/melfore/mosaic#readme",
  "description": "Melfore's UI kit library based on `@material-ui`",
  "repository": {
    "type": "git",
    "url": "https://github.com/melfore/mosaic.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@commitlint/prompt-cli": "^18.2.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/material-icons": "^5.0.11",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.14.16",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.1",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "@storybook/addon-actions": "^7.5.2",
    "@storybook/addon-coverage": "^0.0.9",
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-interactions": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/addon-themes": "^7.6.6",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.5.2",
    "@storybook/react-webpack5": "^7.5.2",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.2",
    "@types/luxon": "^3.3.3",
    "@types/mdx": "^2.0.9",
    "@types/node": "^18.18.8",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@types/react-test-renderer": "^18.0.5",
    "@types/react-window": "^1.8.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "commitizen": "^4.3.0",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "react-window": "^1.8.9",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.6",
    "storybook": "^7.5.2",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@emotion/react": ">= 11.8.2 < 12",
    "@emotion/styled": ">= 11.8.1 < 12",
    "@mui/icons-material": ">= 5.5.1 < 6",
    "@mui/material": ">= 5.5.3 < 6",
    "@mui/x-date-pickers": ">= 6.16.0 < 7",
    "luxon": ">= 3.4.3 < 4",
    "react": ">= 18 < 19",
    "react-dom": ">= 18 < 19",
    "react-window": ">= 1.8.0 < 1.9.0"
  },
  "scripts": {
    "build": "npm run pre:build && tsc",
    "build:storybook": "storybook build -o docs/latest --docs",
    "build:storybook:beta": "storybook build -o docs/beta --docs",
    "build:test": "npm run pre:test && tsc",
    "build:yalc": "npm run build && yalc publish --push --private",
    "clean:code": "eslint ./ --ext .js,.jsx,.ts,.tsx --fix && prettier ./ --write",
    "clean:packages": "rimraf package-lock.json",
    "clean:workspace": "rimraf coverage/ && rimraf dist && rimraf tsconfig.json && rimraf node_modules/",
    "commit": "npx git-cz",
    "pre:build": "rimraf ./dist && copyfiles -a -f ./.typescript/build/tsconfig.json ./",
    "pre:commit": "npm run clean:code",
    "pre:push": "npm run build",
    "pre:test": "copyfiles -a -f ./.typescript/test/tsconfig.json ./",
    "prepare": "husky install",
    "start": "npm run pre:test && storybook dev -p 6006",
    "start:ci": "npm run pre:test && storybook dev -p 6006 --docs --ci",
    "test": "test-storybook --watch",
    "test:ci": "test-storybook --coverage --coverageDirectory coverage/storybook --ci --verbose",
    "test:coverage": "test-storybook --coverage --coverageDirectory coverage/storybook",
    "test:report": "npx nyc report --reporter=lcov -t coverage/storybook --report-dir coverage/storybook",
    "upload": "npm run build && npm publish --access public",
    "update:packages": "npx npm-check-updates --interactive --upgrade --doctor --doctorTest 'npm run pre:push'",
    "update:repo:from:beta": "git checkout origin/beta -- CHANGELOG.md && git checkout origin/beta -- docs/beta"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "license": "MIT"
}
