{
  "name": "@melfore/mosaic",
  "version": "7.6.6",
  "homepage": "https://github.com/melfore/mosaic#readme",
  "description": "Melfore's UI kit library based on `@material-ui`",
  "repository": {
    "type": "git",
    "url": "https://github.com/melfore/mosaic.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@cfaester/enzyme-adapter-react-18": "^0.7.0",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@commitlint/prompt-cli": "^17.6.1",
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@storybook/addon-actions": "^7.0.7",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addon-mdx-gfm": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-webpack5": "^7.0.7",
    "@types/enzyme": "^3.10.13",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^29.5.1",
    "@types/mdx": "^2.0.5",
    "@types/node": "^16.18.25",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "semantic-release": "^19.0.5",
    "storybook": "^7.0.7",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "@emotion/react": ">= 11.8.2 < 12",
    "@emotion/styled": ">= 11.8.1 < 12",
    "@mui/icons-material": ">= 5.5.1 < 6",
    "@mui/material": ">= 5.5.3 < 6",
    "react": ">= 18 < 19",
    "react-dom": ">= 18 < 19"
  },
  "scripts": {
    "build": "npm run pre:build && tsc",
    "build:storybook": "storybook build -o docs/latest --docs",
    "build:storybook:beta": "storybook build -o docs/beta --docs",
    "build:test": "npm run pre:test && tsc",
    "build:yalc": "npm run build && yalc publish --push --private",
    "clean:code": "eslint '**/*.ts*' --fix && prettier --write .",
    "clean:packages": "rm -rf package-lock.json",
    "clean:workspace": "rm -rf coverage/ && rm -rf dist && rm -rf tsconfig.json && rm -rf node_modules/",
    "commit": "npx git-cz",
    "pre:build": "rm -rf ./dist && cp -f ./.typescript/tsconfig.build.json ./tsconfig.json",
    "pre:commit": "npm run clean:code && npm run build:test",
    "pre:push": "npm run build && CI=true npm run test:coverage",
    "pre:test": "cp -f ./.typescript/tsconfig.test.json ./tsconfig.json",
    "prepare": "husky install",
    "start": "npm run pre:test && storybook dev -p 9009",
    "test": "npm run pre:test && jest --clearCache && jest --c ./.jest/config.js --watch",
    "test:coverage": "npm run pre:test && jest --clearCache && jest --c './.jest/config.js' --coverage",
    "upload": "npm run build && npm publish --access public",
    "update:packages": "npx npm-check-updates --interactive --upgrade --doctor --doctorTest 'npm run pre:push'",
    "update:repo:from:beta": "git checkout origin/beta -- CHANGELOG.md && git checkout origin/beta -- docs/beta"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "license": "MIT"
}
