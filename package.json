{
  "name": "@melfore/mosaic",
  "version": "7.3.0",
  "homepage": "https://github.com/melfore/mosaic#readme",
  "description": "Melfore's UI kit library based on `@material-ui`",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melfore/mosaic.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/prompt-cli": "^17.1.2",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^29.0.2",
    "@types/mdx": "^2.0.2",
    "@types/node": "^14.18.29",
    "@types/react": "^16.14.32",
    "@types/react-dom": "^16.9.16",
    "@types/react-test-renderer": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-storybook": "^0.6.4",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "prettier": "^2.7.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "@material-ui/core": ">= 4.12.3 < 5",
    "@material-ui/icons": ">= 4.11.2 < 5",
    "@material-ui/lab": ">= 4.0.0-alpha.57 < 5",
    "react": ">= 16.13.0 < 17",
    "react-dom": ">= 16.13.0 < 17"
  },
  "scripts": {
    "build": "npm run pre:build && tsc",
    "build:storybook": "build-storybook -o docs/latest --docs",
    "build:test": "npm run pre:test && tsc",
    "build:yalc": "npm run build && yalc publish --push --private",
    "clean:code": "eslint '**/*.ts*' --fix && prettier --write .",
    "clean:packages": "rm -rf package-lock.json",
    "clean:workspace": "rm -rf coverage/ && rm -rf dist && rm -rf tsconfig.json && rm -rf node_modules/",
    "commit": "npx git-cz",
    "pre:build": "rm -rf ./dist && cp -f ./.typescript/tsconfig.build.json ./tsconfig.json",
    "pre:test": "cp -f ./.typescript/tsconfig.test.json ./tsconfig.json",
    "prepare": "husky install",
    "start": "npm run pre:test && start-storybook -p 9009",
    "test": "npm run pre:test && jest --clearCache && jest --c ./.jest/config.js --watch",
    "test:coverage": "npm run pre:test && jest --clearCache && jest --c './.jest/config.js' --coverage",
    "upload": "npm run build && npm publish --access public",
    "update:with:beta": "git checkout origin/beta -- CHANGELOG.md && git checkout origin/beta -- docs/beta"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "license": "MIT",
  "dependencies": {}
}
