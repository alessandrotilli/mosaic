{
  "name": "@melfore/mosaic",
  "version": "6.5.0-beta.1",
  "homepage": "https://github.com/melfore/mosaic#readme",
  "description": "Melfore's UI kit library based on `@material-ui`",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melfore/mosaic.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt-cli": "^16.2.3",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/lab": "^5.0.0-alpha.75",
    "@mui/material": "^5.5.3",
    "@mui/styles": "^5.5.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/react": "^6.4.21",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^27.4.1",
    "@types/mdx": "^2.0.1",
    "@types/node": "^14.18.12",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.8",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "@emotion/react": ">= 11.8.2 < 12",
    "@emotion/styled": ">= 11.8.1 < 12",
    "@mui/icons-material": ">= 5.5.1 < 6",
    "@mui/lab": ">= 5.0.0-alpha.75 < 6",
    "@mui/material": ">= 5.5.3 < 6",
    "@mui/styles": ">= 5.5.3 < 6",
    "react": ">= 17 < 18",
    "react-dom": ">= 17 < 18"
  },
  "scripts": {
    "align:master": "git checkout origin/beta -- CHANGELOG.md && git checkout origin/beta -- docs/beta",
    "build": "npm run pre:build && tsc",
    "build:storybook": "build-storybook -o docs/latest --docs",
    "build:storybook:beta": "build-storybook -o docs/beta --docs",
    "build:test": "npm run pre:test && tsc",
    "build:yalc": "npm run build && yalc publish --push --private",
    "clean:code": "eslint '**/*.ts*' --fix && prettier --write .",
    "clean:packages": "rm -rf package-lock.json",
    "clean:workspace": "rm -rf coverage/ && rm -rf dist && rm -rf tsconfig.json && rm -rf node_modules/",
    "commit": "npx git-cz",
    "pre:build": "rm -rf ./dist && cp -f ./.typescript/tsconfig.build.json ./tsconfig.json",
    "pre:test": "cp -f ./.typescript/tsconfig.test.json ./tsconfig.json",
    "prepare": "husky install",
    "start": "npm run pre:test && start-storybook -p 9009",
    "test": "npm run pre:test && jest --clearCache && jest --c ./.jest/config.js --watch",
    "test:coverage": "npm run pre:test && jest --clearCache && jest --c './.jest/config.js' --coverage",
    "upload": "npm run build && npm publish --access public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "license": "MIT"
}
